<div class="row-fluid">
  <div class="span6 offset3 well">
    <h3 class="text-center">Browse by Category</h3>
    <div class="text-center">
    | <% @categories.each do |category| %>
        <%= link_to category.name, '/search/by_category?category='+category.name %> |
      <% end %>
    </div>
    <br/>
    <h3 class="text-center">Browse by Instrumentation/Voicing</h3>
    <div class="text-center">
    | <% @voicings.each do |voicing| %>
        <%= link_to voicing.name, '/search/by_voicing?voicing='+voicing.name %> |
      <% end %>
    </div>
  </div>
</div>
